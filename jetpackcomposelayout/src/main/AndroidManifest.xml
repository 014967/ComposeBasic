<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpackcomposelayout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeBasic">
        <activity
            android:name=".ConstraintLayoutActivity"
            android:exported="false"
            android:label="@string/title_activity_constraint_layout"
            android:theme="@style/Theme.ComposeBasic" />
        <activity
            android:name=".StaggeredGridActivity"
            android:exported="false"
            android:label="@string/title_activity_staggered_grid"
            android:theme="@style/Theme.ComposeBasic" />
        <activity
            android:name=".CustomLayoutActivity"
            android:exported="false"
            android:label="@string/title_activity_custom_layout"
            android:theme="@style/Theme.ComposeBasic" />
        <activity
            android:name=".WorkingListActivity"
            android:exported="false"
            android:label="@string/title_activity_working_list"
            android:theme="@style/Theme.ComposeBasic" />
        <activity
            android:name=".SlotApiActivity"
            android:exported="false"
            android:label="@string/title_activity_slot_api"
            android:theme="@style/Theme.ComposeBasic" />
        <activity
            android:name=".modifier"
            android:exported="false"
            android:label="@string/title_activity_modifier"
            android:theme="@style/Theme.ComposeBasic" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeBasic">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>